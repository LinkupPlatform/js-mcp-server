{
  "name": "linkup-mcp-server",
  "version": "1.0.5",
  "description": "'Linkup MCP server'",
  "types": "build/index.d.ts",
  "bin": {
    "linkup-mcp-server": "build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 766 build/index.js",
    "commitlint": "commitlint --edit",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "husky",
    "prune": "knip",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "linkup",
    "mcp",
    "llm",
    "nodejs",
    "client",
    "search"
  ],
  "author": "Linkup",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "commander": "^13.1.0",
    "linkup-sdk": "^1.0.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cli": "^19.8.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.61.2",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failTitle": "The automated release is failing ðŸš¨",
          "failComment": false,
          "labels": false,
          "releasedLabels": false
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
